# Julia

snippet todo
	# TODO: $1

snippet fix
	# FIX: $1

snippet note
	% NOTE: $1

snippet if
	if $1
		${0:${VISUAL}}
	end

snippet ife
	if $1
		${2: # condition_1}
	else
		${0: # else}
	end

snippet wh
	while $1
		${0:${VISUAL}}
	end

snippet for
	for ${1:i}=${2:1}:${3:10}
		${0:# loop}
	end

snippet ret
	return ${0}
